{
  "name": "web-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm tailwind && next dev",
    "build": "paraglide-js compile --project ./project.inlang && next build",
    "start": "pnpm tailwind && next start",
    "lint": "next lint",
    "postinstall": "pnpm tailwind && paraglide-js compile --project ./project.inlang",
    "tailwind": "tailwindcss -i ./app/input.css -o ./app/dist.css -c tailwind.config.js"
  },
  "dependencies": {
    "react": "^18",
    "react-dom": "^18",
    "next": "14.1.0",
    "@octokit/core": "^5.1.0",
    "use-one": "^1.1.1",
    "immer": "^10.0.3",
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^11.0.3",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "tailwindcss-animate-plugin": "^1.1.0"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "@inlang/paraglide-js": "1.2.5"
  }
}
